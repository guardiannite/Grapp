@model Grapp.Models.CharacterHistoryModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>Skill</th>
        <th>Level</th>
        <th>Experience</th>
    </tr>
    @foreach (var skill in Model.Skills)
    {
        <tr>
            <td>@skill.Name</td>
            <td>@skill.Level.ToString("#,##0")</td>
            <td>@skill.Experience.ToString("#,##0")</td>
        </tr>
    }
</table>

